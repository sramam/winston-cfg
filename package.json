{
  "name": "winston-cfg",
  "version": "1.0.11",
  "description": "Enables [winston](https://github.com/winstonjs/winston) configuration via [node-config](https://github.com/lorenwest/node-config)",
  "keywords": [
    "TypeScript",
    "winston",
    "config",
    "file"
  ],
  "author": "Shishir Ramam",
  "repository": "sramam/winston-cfg",
  "license": "Apache-2.0",
  "private": false,
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "scripts"
  ],
  "scripts": {
    "staged-mods-ok": "git-mods --staged_ok",
    "no-mods": "git-mods",
    "clean": "rimraf build coverage",
    "clean:dist": "rimraf dist",
    "clean:ts-cover": "rimraf coverage/typescript",
    "init:ts-cover": "mkdirp coverage/typescript",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "compile:build": "tsc",
    "compile:dist": "tsc -p tsconfig.dist.json",
    "compile:watch": "tsc -w",
    "sh": "npm-sh",
    "secure": "nsp check",
    "check": "npm outdated",
    "coverage": "run-batch",
    "coverage:dist": "run-batch",
    "remap-html": "remap-istanbul -i coverage/coverage.json -o coverage/typescript -t html",
    "remap-json": "remap-istanbul -i coverage/coverage.json -o coverage/typescript/coverage.json -t json",
    "remap-lcov": "remap-istanbul -i coverage/coverage.json -o coverage/typescript/lcov.info -t lcovonly",
    "coverage-report": "istanbul report --include=coverage/typescript/coverage.json text-summary",
    "check-coverage": "istanbul check-coverage --include coverage/typescript/coverage.json",
    "test-with-cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- --opts mocha.opts",
    "test-without-cover": "mocha --opts mocha.opts",
    "build": "npm-run-batch",
    "build:dist": "run-batch",
    "build:watch": "cross-env NODE_ENV=production run-batch",
    "test": "run-batch",
    "test:dist": "run-batch",
    "pkg-ok": "pkg-ok",
    "precommit": "run-batch",
    "prepush": "run-batch",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "run-batch": {
    "build": [
      "clean",
      "lint",
      "compile:build",
      "test"
    ],
    "build:watch": [
      "compile:watch"
    ],
    "build:dist": [
      "lint",
      "clean",
      "compile:build",
      "test:dist",
      "clean:dist",
      "compile:dist",
      "pkg-ok"
    ],
    "test:dist": [
      "test-with-cover",
      "coverage:dist"
    ],
    "test": [
      "test-with-cover",
      "coverage"
    ],
    "coverage": [
      "clean:ts-cover",
      "init:ts-cover",
      "remap-html",
      "remap-json",
      "coverage-report"
    ],
    "coverage:dist": [
      "clean:ts-cover",
      "init:ts-cover",
      "remap-html",
      "remap-json",
      "coverage-report",
      "check-coverage"
    ],
    "precommit": [
      "postinstall",
      "build:dist",
      "staged-mods-ok"
    ],
    "prepush": [
      "build:dist",
      "secure",
      "no-mods"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/circular-json": "^0.4.0",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.51",
    "@types/semver": "^5.3.29",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "commander": "^2.9.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "git-mods": "^1.0.0",
    "graceful-fs": "^4.1.9",
    "husky": "^0.14.3",
    "istanbul": "^0.4.4",
    "mocha": "^4.0.1",
    "node-emoji": "^1.4.1",
    "npm-check-updates": "^2.13.0",
    "npm-run-batch": "0.0.6",
    "npm-sh": "^1.0.1",
    "nsp": "^3.1.0",
    "nsp-formatter-checkstyle": "^1.0.1",
    "nsp-formatter-remarkup": "^1.0.3",
    "pkg-ok": "^1.0.1",
    "remap-istanbul": "^0.9.1",
    "rimraf": "^2.6.2",
    "shelljs-nodecli": "^0.1.1",
    "std-mocks": "^1.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "config": "^1.28.1",
    "lodash.merge": "^4.6.0",
    "mkdirp": "^0.5.1",
    "parse-git-config": "^1.1.1",
    "semver": "^5.3.0",
    "winston": "^2.4.0"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
